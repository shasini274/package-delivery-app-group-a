<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.manager.vendor.VendorProfileFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:orientation="vertical">

        <TextView
            android:id="@+id/vendor_profile_name"
            style="@style/ProfileStyle" />

        <TextView
            android:id="@+id/vendor_profile_address"
            style="@style/ProfileStyle" />

        <TextView
            android:id="@+id/vendor_profile_email"
            style="@style/ProfileStyle" />

        <TextView
            android:id="@+id/vendor_profile_person"
            style="@style/ProfileStyle" />

        <TextView
            android:id="@+id/vendor_profile_number"
            style="@style/ProfileStyle" />

        <TextView
            android:id="@+id/vendor_profile_id"
            style="@style/ProfileStyle" />

    </LinearLayout>
</FrameLayout>